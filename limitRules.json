{
  "freepik": {
    "alias": "freepik",
    "host": "freepik.com",
    "method": "request",
    "updateOnNewTab": false,
    "triggers": {
      "\\/api\\/regular\\/download\\?resource=\\d+&action=download(?:&.*)?": {
        "type": "fetch",
        "from": "^https:\\/\\/[^/]*freepik\\.com\\/",
        "marker": "",
        "whitelist": [
          "/*freepik.com/user/downloads"
        ]
      }
    },
    "confirm": {
    },
    "block": {
      "requests": [
        "\\/api\\/regular\\/download\\?resource=\\d+&action=download(?:&.*)?"
      ]
    },
    "tokens": {
      "tokensDefaultAmount": 1,
      "tokensType": "fixed",
      "counter": ""
    },
    "limits": {
      "hourly": -1,
      "daily": 2,
      "weekly": -1,
      "monthly": -1
    }
  },
